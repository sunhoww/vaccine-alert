{
  "name": "vaccine-alert",
  "version": "0.3.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "serve": "node dist/index.js",
    "start": "NODE_ENV=production APP_VERSION=$npm_package_version yarn serve",
    "watch-node": "nodemon dist/index.js",
    "watch-ts": "tsc -w",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\""
  },
  "author": "Sun Howwrongbum <sun@libermatic.com>",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@types/node-fetch": "^2.5.10",
    "@types/object-hash": "^2.1.0",
    "concurrently": "^6.1.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "tslint": "^6.1.3",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "dotenv": "^9.0.2",
    "node-fetch": "^2.6.1",
    "object-hash": "^2.1.1"
  }
}
